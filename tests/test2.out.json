{
  "network_interface": "localhost",
  "conda": "hashserver",
  "port_start": 10000,
  "port_end": 19999,
  "timeout": 600,
  "workdir": "~/seamless-buffers/myproject",
  "key": "hashserver-local-rw-{\"/myproject\".strip(\"/\").replace(\"/\", \"--\")}",
  "command": "hashserver --port-range {port_start} {port_end} --status-file {status_file} --host {config['network_interface']} --timeout {timeout} {workdir}{' --writable' if 'rw' == 'rw' else ''}",
  "handshake": "healthcheck"
}
